# top-most EditorConfig file
root = true

# Matches every file with brace expansion notation
[*]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
# Set default charset
charset = utf-8
# Tab indentation
indent_style = tab
# Delete white spaces before new line
trim_trailing_whitespace = true
